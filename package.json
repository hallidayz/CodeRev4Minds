{
  "name": "code-rev-minds-react-app",
  "version": "1.0.0",
  "description": "AI-powered code review automation tool - Complete React application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production tsc && vite build",
    "build:prod": "chmod +x scripts/build-production.sh && ./scripts/build-production.sh",
    "preview": "NODE_ENV=production vite preview --https",
    "start": "NODE_ENV=production vite preview --https --port 3000",
    "test:build": "node scripts/test-production.js",
    "test:production": "npm run build && npm run test:build && npm run preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.44.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "keywords": [
    "ai",
    "code-review",
    "automation",
    "react",
    "typescript",
    "saas"
  ],
  "author": "AC MiNDS, LLC",
  "license": "PROPRIETARY",
  "copyright": "Copyright (c) 2024 AC MiNDS, LLC. All rights reserved."
}
